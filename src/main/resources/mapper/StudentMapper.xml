<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zbin.cisp.dao.StudentMapper">
  <select id="selectById" resultType="com.zbin.cisp.domain.Student">
    select * from student where id = #{id}
  </select>

  <insert id="insert" parameterType="com.zbin.cisp.domain.Student">
    insert into student
    values (#{id},#{name},#{age})
  </insert>
</mapper>